<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="385.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <Button fx:id="btnLoadEkra" layoutX="1.0" layoutY="5.0" mnemonicParsing="false" onAction="#btnLoadPathToEkraClick" prefHeight="25.0" prefWidth="278.0" text="Загрузить файл конфигурации терминала Экра" />
      <TextField fx:id="panel" layoutX="3.0" layoutY="87.0" prefHeight="25.0" prefWidth="444.0" promptText="панель, короткое имя префикса панели для имени переменной scada" />
      <TextField fx:id="driverName" layoutX="3.0" layoutY="112.0" prefHeight="25.0" prefWidth="444.0" promptText="имя драйвера в scada" />
      <TextField fx:id="tagnamePrefix" layoutX="3.0" layoutY="137.0" prefHeight="25.0" prefWidth="444.0" promptText="Префикс для дисп наименования переменной в scada" />
      <TextField fx:id="netAddr" layoutX="3.0" layoutY="162.0" prefHeight="25.0" prefWidth="444.0" promptText="NetAddress для переменных" />
      <Button fx:id="startBtn" layoutX="3.0" layoutY="187.0" mnemonicParsing="false" onAction="#startGenerate" prefHeight="25.0" prefWidth="278.0" text="Создать переменные для V460" />
      <Label fx:id="lblTooltip" layoutX="114.0" layoutY="65.0" onMouseClicked="#lblTooltipClick" onMouseExited="#lblTooltipExit" prefHeight="17.0" prefWidth="127.0" text="Посмотреть пример" />
      <Label layoutX="3.0" layoutY="65.0" prefHeight="17.0" prefWidth="111.0" text="Заполните поля" />
      <ListView fx:id="listLog" layoutX="4.0" layoutY="224.0" prefHeight="153.0" prefWidth="444.0" />
      <Label fx:id="txtPathEkra" layoutY="30.0" prefHeight="25.0" prefWidth="442.0">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
   </children>
</Pane>
