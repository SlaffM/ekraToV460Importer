<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <TextField fx:id="panel" promptText="панель, короткое имя префикса панели для имени переменной scada" />
      <TextField fx:id="driverName" promptText="имя драйвера в scada" />
      <TextField fx:id="tagnamePrefix" promptText="Префикс для дисп наименования переменной в scada" />
      <TextField fx:id="netAddr" promptText="NetAddress для переменных" />
      <Button fx:id="startBtn" mnemonicParsing="false" onAction="#startGenerate" prefWidth="200.0" text="start" />
   </children>
</VBox>
